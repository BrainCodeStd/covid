{
  "name": "project34-api",
  "version": "1.0.0",
  "description": "Project34-API",
  "main": "index.js",
  "engines": {
    "node": "12.17.0"
  },
  "scripts": {
    "dev": "nodemon bin/www",  
    "client":"cd Client && npm run start-dev", 
    "homepage": "./Client",
    "start": "concurrently \"nodemon bin/www\" \"npm run client\"",
    "start-dev": "PORT=2000 react-scripts start",    
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass src/assets/scss/material-kit-react.scss dist/material-kit-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "compile-sass": "node-sass src/assets/scss/material-kit-react.scss src/assets/css/material-kit-react.css",
    "minify-sass": "node-sass src/assets/scss/material-kit-react.scss src/assets/css/material-kit-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/material-kit-react.scss src/assets/css/material-kit-react.css --source-map true"
  },
  "author": "Special Man Global Solution LTD",
  "license": "ISC",
  "dependencies": {
    "@date-io/date-fns": "^2.8.0",
    "@material-ui/core": "4.10.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.2",
    "classnames": "2.2.6",
    "express-favicon": "^2.0.1",
    "history": "4.10.1",
    "node-sass": "4.14.1",
    "nouislider": "14.5.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-cookie": "^4.0.3",
    "react-datetime": "2.16.3",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-slick": "0.26.1",
    "react-swipeable-views": "0.13.9",
    "react-switch": "^5.0.1",
    "react-table": "^6.11.4",
    "react-table-6": "^6.11.0",
    "react-toastify": "^6.0.5",
    "use-force-update": "^1.0.7",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.13",
    "express-formidable": "^1.2.0",
    "express-validator": "^6.6.0",
    "http-errors": "^1.7.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "mongoose": "^5.9.19",
    "mongoose-double": "0.0.1",
    "morgan": "^1.10.0",
    "node-mailjet": "^3.3.1",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "pug": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  }
}
